public class aula05 {
	public static void main(String args[]) {

		String[] meses = new String[11];
		meses[0] = "Janeiro";
		meses[1] = "Feverieiro";
		meses[2] = "Março";
		meses[3] = "Abril";
		meses[4] = "Maio";
		meses[5] = "Junho";
		meses[6] = "Julho";
		meses[7] = "Agosto";
		meses[8] = "Setembro";
		meses[9] = "Outubro";
		meses[10] = "Novembro";
		meses[11] = "Dezembro";

		for (int  j = 0; j<=11;j++ ) {
		System.out.println(j);
} } }
